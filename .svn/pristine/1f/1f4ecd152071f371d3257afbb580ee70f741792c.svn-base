/* 1.搜索块样式 */
.jd_search {
	width: 100%;
	height: 0.35rem;
	position: fixed;
	background-color:#c22600;
	max-width: 6.40rem;
	z-index: 999;
}
.jd_logo {
	width: 56px;
	height: 30px;
	position: absolute;
	top: 8px;
	left: 10px;
	display: block;
	background: url(../img/yuanImg/jd-sprites.png);
	background-size: 200px 200px;
	background-position: 0px -108px;
	
}
.jd_searchBox {
	width: 100%;
	height: 100%;
	padding-left: 76px;
	padding-right: 50px;
	
}
.jd_searchBox>input {
	width: 100%;
	height: 30px;
	margin-top: 5px;
	border-radius: 15px;
	padding-left: 30px;
	color: #666;
}
.jd_searchBox::before {
	content: "";
	width: 27px;
	height: 23px;
	position: absolute;
	background: url(../img/yuanImg/jd-sprites.png);
	background-size: 200px 200px;
	background-position: -56px -108px;
	left: 80px;
	top: 9px;
}
.jd_login {
	width: 40px;
	height: 40px;
	line-height: 40px;
	color: #fff;
	display: block;
	position: absolute;
	top: 0;
	right: 0;
}

/* 2.轮播图样式 */
.top-ol{padding-top: 0.07rem;}
.bac{background:url(../img/navigation/bac01.jpg) no-repeat;}
.circular{
	margin-top: 0.4rem;
	/* 设置圆角边框 四个角 */
	border-top-left-radius: 0.1rem;
	border-top-right-radius: 0.1rem;
	border-bottom-left-radius: 0.1rem;
	border-bottom-right-radius: 0.1rem;
}

/* 导航块样式 */
/* 触屏滑动1 */
.top-navigation{
	padding-top: 0.1rem;
	height: 1.40rem;
	overflow: hidden;
	clear: both;
}
.clearfix{
	float: left;
	width: 20%;
	margin-top: 5px;
	text-align: center;
}
.clearfix a img {
	width: 40px;

}
.clearfix a p{
	line-height: 25px;
	font-size: 13px;
}
.top-slither{
	overflow: hidden;
	clear: both; 
}
.top-slither li{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-flex-wrap: wrap;
	-ms-flex-wrap: wrap;
	/* flex-wrap属性指 轴线换行 (核心)*/
	flex-wrap: wrap; 
	float: left;
}
.dot-container{
    width: 93%;
    background:transparent;
    position: absolute;
    text-align:center;
    bottom: 2.45rem;
}

.dot-container span{
    background-color:#ddd;
    border-radius: 50%;
    width: 0.045rem;
    height: 0.045rem;
    display: inline-block;
    margin:0 0.02rem;
}

.dot-container span.act{
    background-color:#2196f3;
}
/*商品块 样式*/
