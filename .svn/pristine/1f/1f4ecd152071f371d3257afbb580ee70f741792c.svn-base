/* 设置一个样式标记 秒杀块开始 */
.jd_ms > .jd_pTip {
	border-bottom:none;
	padding-left: 0rem;
}
.jd_ms >.jd_pTip >.jd_left >.jd_ms_icon {
	width: 0.16rem;
	height: 0.20rem;
	background: url(../img/yuanImg/seckill-icon.png) no-repeat;
	background-size: 0.16rem 0.2rem;
	margin-top: 0.001rem;
}
.jd_ms > .jd_pTip >.jd_left >.jd_zsms{
	color: #F63515;
	font-size: 0.13rem;
}
.jd_ms >.jd_pTip >.jd_left >.jd_ms_time >span{
	width: 0.13rem;
	height: 0.18rem;
	line-height: 0.18rem;
	color: #fff;
	background-color: #000;
	text-align: center;
	display: inline-block;
}
.jd_ms >.jd_pTip >.jd_left >.jd_ms_time >span:nth-last-of-type(3n){
	color: #000;
	background-color: transparent;
	width: 0.08rem;
}
.top_commodity{
	margin-top: 0.3rem;
	padding-left: 0.1rem;
}
