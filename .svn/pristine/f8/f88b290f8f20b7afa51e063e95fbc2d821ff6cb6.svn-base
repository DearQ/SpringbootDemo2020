.aui-palace {
    padding: 0.5rem 0;
    position: relative;
    overflow: hidden;
}
.aui-palace-grid {
    position: relative;
    float: left;
	right: -2.5rem;
    padding: 1px;
    width: 25%;
    box-sizing: border-box;
    margin: 5px 0;
}
.aui-palace-grid p{
  font-size: 1.4rem;
  color: red;
}
.aui-module-fr {
    position: relative;
    width: 100%;
}
.aui-module-fr:after {
    content: " ";
    display: inline-block;
    height: 8px;
    width: 8px;
    border-width: 2px 2px 0 0;
    border-color: #e93b3d;
    border-style: solid;
	/* transform属性 旋转div的元素 */
    -webkit-transform: rotate(135deg);
    transform: rotate(135deg);
    top: -9px;
    position: absolute;
    top: 50%;
    margin-top: -15px;
    right: -0.5rem;
    border-radius: 1px;
}
