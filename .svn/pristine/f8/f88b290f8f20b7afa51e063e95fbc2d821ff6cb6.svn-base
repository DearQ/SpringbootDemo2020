/* 商品筛选2 样式 */
.n-tabs {
    width: 100%;
    height: 41px;
    overflow: hidden;
    z-index: 1000;
    background-color: #fff;
	border-bottom: 1px #f5f5f5 solid;
}
.n-tabs .n-tabContainer {
    -webkit-overflow-scrolling: touch;
    position: relative;
    top: 0;
    left: 0;
    overflow-x: auto;
    overflow-y: hidden;
    padding-left: 16px;
    height: 51px;
    font-size: 14px;
    color: #333;
    white-space: nowrap;
}
.n-tabs .n-tabContainer .n-tabItem {
    -webkit-box-flex: 1;
    -webkit-flex-grow: 1;
    flex-grow: 1;
    -webkit-flex-shrink: 0;
    flex-shrink: 0;
    display: inline-block;
    height: 25px;
	width: 80px;
    line-height: 25px;
    text-align: center;
    margin-left: 20px;
	background-color: #f2f2f7;
}

.n-tabs .n-tabContainer .n-tabItem:first-child {
    margin-left: 0;
}

.n-tabs .n-tabContainer .n-tabItem .current {
    display: inline-block;
    height: 41px;
    /* border-bottom: 2px solid #ff3542; */
    color: #ff3542;
}

