			  	$("#reduce_num").click(function() {
			  		/* alert(); */
			  		var minus = $(this).next().val();
			  		/* alert(minus); */
			  		if (minus <= 1) {
			  			//alert("不能再减了");
			  			return;
			  		} else {
			  			minus--;
			  		}
			  		$(this).next().val(minus);
			  	});
