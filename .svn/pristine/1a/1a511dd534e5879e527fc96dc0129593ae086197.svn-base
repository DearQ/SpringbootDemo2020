				$(".boxx a").click(function(){
					
					$(this).addClass("imgasfe").siblings().removeClass("imgasfe");
					$(this).removeClass("imgasf").siblings().addClass("imgasf");
					
					let typeId=$(this).children("input").val();
					$.post("/CommtypepricrByprive.do",{"typeId":typeId},function(ret){
						$(".prices").html(ret);
					})
				})

			 	//切换图片
				$(".boxs a").click(function(){
					
					$(this).addClass("imgasfe").siblings().removeClass("imgasfe");
					$(this).removeClass("imgasf").siblings().addClass("imgasf");
					
					let imgId=$(this).children("input").val();
					$(".imgs").html("");
	                var html = "";
				 	$.post("/showImagesByUri.do",{"imgId":imgId},function(ret){
						//console.log(ret)
						$.each(ret,function(i, info){
							html += "<li class='cur'>";
							html += '<a href="javascript:void(0);" rel="'+info.uriDZ+'">';
							html += "<img src='"+info.uriX+"'";
							html += "</li>";
						})
						//html = html.replace(/"/g, "'");
						$(".imgs").append(html);		 
						//alert(html)
					})  
					
				}) 
				//加入购物车
				$(".che").click(function(){
					var name=$(".c_name").html();
					var price=$(".c_discountprice").html();
					var img=$(".imgs li").eq(0).children("a").children("img")[0].src;
					var cid=$(".c_id").val();
					var colour=$(".boxs a[class='imgasfe']").children("span").html();
					var edition=$(".boxx a[class='imgasfe']").children("span").html();
					var amount=$(".amount").val();
					//alert(img)
					var data={
							"name":name,
							"price":price,
							"img":img,
							"cid":cid,
							"colour":colour,
							"edition":edition,
							"amount":amount
					}
					$.post("/addShoopc.do",data,function(ret){
						//console.log(ret)
						if(ret=="uidNo"){
							alert("请先登录！");
						}else if(ret=="ok"){
							alert("加入购物车成功！");
						}else{
							alert("系统异常,加入购物车失败！");
						}
					})
				}) 
				
			})
			
