/* 鍙充晶瀵艰埅鏉� */

.toolbars {
	font-size: 0.14rem;
	position: fixed;
	top: 0;
	right: 0;
	height: 100%;
	width: 0.06rem;
	background-color: #7a6e6e;
}

.toolbars ul {
	position: absolute;
	top: 50%;
	right: 0;
	transform: translate(0, -40%);
	z-index: 3;
}

.toolbars ul li {
	width: 0.34rem;
	height: 0.35rem;
	margin-bottom: 0.02rem;
	background-color: #7A6E6E;
	position: relative;
}

.toolbars ul .jdvip .toolbars-ico {
	display: block;
	width: 0.34rem;
	height: 0.35rem;
	background: url(../img/toolbars.png)no-repeat -0.88rem -1.75rem;
}

.toolbars ul .cart .toolbars-ico {
	display: block;
	width: 0.34rem;
	height: 0.35rem;
	background: url(../img/toolbars.png)no-repeat -0.5rem 0;
}

.toolbars ul .follow .toolbars-ico {
	display: block;
	width: 0.34rem;
	height: 0.35rem;
	background: url(../img/toolbars.png)no-repeat -0.5rem -0.5rem;
}

.toolbars ul .message .toolbars-ico {
	display: block;
	width: 0.34rem;
	height: 0.35rem;
	background: url(../img/toolbars.png)no-repeat -1.9rem -1.5rem;
}

.toolbars ul .jimi .toolbars-ico {
	display: block;
	width: 0.34rem;
	height: 0.35rem;
	background: url(../img/we.png)no-repeat -0.03rem 0rem;
}

.toolbars ul .feedback .toolbars-ico {
	display: block;
	width: 0.34rem;
	height: 0.35rem;
	background: url(../img/toolbars.png)no-repeat -0.5rem -3rem;
}

.toolbars ul .jdvip i {
	position: absolute;
	top: 0.05rem;
	right: 0.08rem;
	width: 0.06rem;
	height: 0.06rem;
	background: url(../img/toolbars.png)no-repeat -1.5rem -1.5rem;
}

.toolbars-footer {
	position: absolute;
	bottom: 0.5rem;
	right: 0;
	background-color: #7A6E6E;
	width: 0.34rem;
	height: 0.35rem;
	z-index: 3;
}

.toolbars-footer ul .headpiece .toolbars-ico {
	display: block;
	width: 0.34rem;
	height: 0.35rem;
	background: url(../img/toolbars.png)no-repeat -0.5rem -2.5rem;
}

/*榧犳爣鎮诞鍚庝几鍑虹殑閮ㄥ垎*/
.toolbars .flex {
	position: absolute;
	top: 0;
	right: 0.34rem;
	width: 0;
	overflow: hidden;
	height: 0.35rem;
	line-height: 0.35rem;
	background-color: #b61d1d;
	text-align: center;
	color: #fff;
	z-index: -1;
	transition: width .3s;
}

.toolbars .flex1 {
	display: none;
	position: absolute;
	bottom: 0;
	right: 0.38rem;
	width: 1.2rem;
	height: 1.6rem;
	font-size: 0.14rem;
	line-height: 0.16rem;
	color: rgb(255, 255, 255);
	text-align: center;
	padding: 0.08rem 0.1rem;
	border-radius: 0.04rem;
	overflow: hidden;
	background: rgb(228, 57, 60);
	text-align: center;
	z-index: -1;
	transition: width .3s;
}

/*榧犳爣鎮诞鍚庣殑鏁堟灉*/
.toolbars ul li:hover {
	background-color: #b61d1d;
}

.toolbars ul li:hover .flex {
	width: 0.62rem;
	display: block;
	background-color: #b61d1d;
}

.toolbars li:hover .flex1 {
	width: 1.2rem;
	display: block;
	background: rgb(228, 57, 60);
}

.toolbars ul li:hover i {
	display: none;
}

