}

#backward {
	display: block;
	position: absolute;
	left: 0;
	top: 0;
	width: 0.17rem;
	height: 0.54rem;
	background: url("../img/newicon.png") -0.68rem -1.39rem no-repeat;
	margin-right: 0.08rem;
	cursor: default;
}

#forward {
	display: block;
	width: 0.17rem;
	height: 0.54rem;
	position: absolute;
	top: 0;
	right: 0;
	background: url("../img/newicon.png") -0.51rem -1.39rem no-repeat;
	cursor: pointer;
}

#backward.on {
	background-position: -0.54rem -1.39rem;
	cursor: pointer;
}

#backward.off {
	background-position: -0.68rem -1.39rem;
	cursor: default;
}

#forward.on {
	background-position: -0.51rem -1.39rem;
	cursor: pointer;
}

#forward.off {
	background-position: -0.85rem -1.39rem;
	cursor: default;
}

