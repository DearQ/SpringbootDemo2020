* {
	margin: 0px;
	padding: 0px;
}

body,
html {
	font-size: 100px;
}

ul li {
	list-style: none;
}

#g-top {
	font-size: 0.14rem;
	border-bottom: 0.01rem solid #ddd;
	background-color: #e3e4e5;
}

#g-top ul li {
	float: left;
	padding-top: 0.1rem;
}

#g-top ul.g-left li,
#g-top ul.g-right li {
	margin-left: 0.2rem;
}

#g-top ul li a {
	color: #999;
}

#g-top a:hover {
	color: #E1251B;
	text-decoration: none;
}

/* 鎴戠殑 */
#g-top ul li.my {
	position: relative;
}

#g-top ul li.my a span {
	width: 0.38rem;
	background-position: 0rem -0.68rem;
}

#g-top ul li.my a span,
#g-top .g-hui {
	display: inline-block;
	height: 0.16rem;
	background-image: url(../img/user-plus-status.png);

}

#g-top .g-hui {
	width: 0.8rem;
	background-position: 0rem -0.34rem;
}

#g-top ul li.my .m-main {
	position: absolute;
	width: 2.8rem;
	/* height: 1rem; */
	z-index: 55;
	top: 0.4rem;
	display: none;
	background-color: #FFFFFF;
	border: 0.01rem solid #CCCCCC;
	box-shadow: 0.01rem 0.02rem 0.01rem rgba(0, 0, 0, .1);
}

#g-top ul .my:hover .m-main {
	display: block;
}

#g-top .m-down li a.als span {
	display: block;
	width: 0.56rem;
	height: 0.56rem;
	background-image: url(../img/sprite.shortcutUserinfo.png);

}

#g-top .m-down li a.m1 span {
	background-position: -0.56rem 0rem;
}

#g-top .m-down li a.m2 span {
	background-position: -1.11rem -1.67rem;
}

#g-top .m-down li a.m3 span {
	background-position: -1.69rem -0.55rem;
}

#g-top .m-down li a.m4 span {
	background-position: -0.55rem -1.11rem;
}

#g-top .m-down li a.m5 span {
	background-position: -1.67rem 0rem;
}

#g-top .m-down li a.m6 span {
	background-position: -1.67rem -1.11rem;
}

#g-top .m-down li a.m7 span {
	background-position: -0.55rem -1.67rem;
}

#g-top .m-down li a.m8 span {
	background-position: -0.55rem -0.55rem;
}

#g-top ul.g-right li.hidetu {
	width: 0.84rem;
}

#g-top .c {
	width: 2.8rem;
	height: 1rem;
	/* background: wheat; */
	overflow: hidden;
	position: relative;
}

#g-top .left-arrow {
	left: 0rem;
	background: url(../img/left-arrow-down.PNG) no-repeat scroll center center transparent;
}

#g-top .right-arrow {
	right: 0rem;
	background: url(../img/right-arrow-down.PNG) no-repeat scroll center center transparent;
}

#g-top .c .left-arrow,
#g-top .c .right-arrow {
	width: 0.2rem;
	height: 0.4rem;
	/* background-color: coral; */
	cursor: pointer;
	position: absolute;
	top: 0.25rem;
	line-height: 0.4rem;
	z-index: 9;
	display: block;
}

.left-arrow:hover,
.right-arrow:hover {
	background-color: rgba(0, 0, 0, 0.1);
}

#g-top .hu_c {
	width: 8.4rem;
	height: 1rem;
	/* background-color: pink; */
	position: absolute;
}

#g-top .m-down {
	width: 2.8rem;
	height: 0.74rem;
	float: left;
	padding: 0.1rem;
}

/* 瀹㈡埛鏈嶅姟 */
#g-top ul li.kefu {
	width: 0.83rem;
	height: 0.3rem;
}

#g-top ul li.kefu span {
	padding-left: 0.1rem;
	color: #666;
}

/* 缃戠珯瀵艰埅 */
#g-top ul li.wangzhan {
	width: 0.73rem;
	height: 0.3rem;
	/*border: 1px solid orange;*/
}

