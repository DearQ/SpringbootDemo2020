* {
	margin: 0;
	padding: 0;
}

body,
html {
	font-size: 100px;
}

ul li {
	list-style: none;
}

#g-top {
	font-size: 0.14rem;
	border-bottom: 0.01rem solid #ddd;
	background-color: #e3e4e5;
}

#g-top ul li {
	float: left;
	padding-top: 0.1rem;
}

#g-top ul.g-left li,
#g-top ul.g-right li {
	margin-left: 0.2rem;
}

#g-top ul li a {
	color: #999;
}

#g-top a:hover {
	color: #E1251B;
	text-decoration: none;
}

/* 我的 */
#g-top ul li.my {
	position: relative;
}

#g-top ul li.my a span {
	width: 0.38rem;
	background-position: 0rem -0.68rem;
}

#g-top ul li.my a span,
#g-top .g-hui {
	display: inline-block;
	height: 0.16rem;
	background-image: url(../img/user-plus-status.png);

}

#g-top .g-hui {
	width: 0.8rem;
	background-position: 0rem -0.34rem;
}

#g-top ul li.my .m-main {
	position: absolute;
	width: 2.8rem;
	/* height: 1rem; */
	z-index: 55;
	top: 0.4rem;
	display: none;
	background-color: #FFFFFF;
	border: 0.01rem solid #CCCCCC;
	box-shadow: 0.01rem 0.02rem 0.01rem rgba(0, 0, 0, .1);
}

#g-top ul .my:hover .m-main {
	display: block;
}

#g-top .m-down li a.als span {
	display: block;
	width: 0.56rem;
	height: 0.56rem;
	background-image: url(../img/sprite.shortcutUserinfo.png);

}

#g-top .m-down li a.m1 span {
	background-position: -0.56rem 0rem;
}

#g-top .m-down li a.m2 span {
	background-position: -1.11rem -1.67rem;
}

#g-top .m-down li a.m3 span {
	background-position: -1.69rem -0.55rem;
}

#g-top .m-down li a.m4 span {
	background-position: -0.55rem -1.11rem;
}

#g-top .m-down li a.m5 span {
	background-position: -1.67rem 0rem;
}

#g-top .m-down li a.m6 span {
	background-position: -1.67rem -1.11rem;
}

#g-top .m-down li a.m7 span {
	background-position: -0.55rem -1.67rem;
}

#g-top .m-down li a.m8 span {
	background-position: -0.55rem -0.55rem;
}

#g-top ul.g-right li.hidetu {
	width: 0.84rem;
}

#g-top .c {
	width: 2.8rem;
	height: 1rem;
	/* background: wheat; */
	overflow: hidden;
	position: relative;
}

#g-top .left-arrow {
	left: 0rem;
	background: url(../img/left-arrow-down.PNG) no-repeat scroll center center transparent;
}

#g-top .right-arrow {
	right: 0rem;
	background: url(../img/right-arrow-down.PNG) no-repeat scroll center center transparent;
}

#g-top .c .left-arrow,
#g-top .c .right-arrow {
	width: 0.2rem;
	height: 0.4rem;
	/* background-color: coral; */
	cursor: pointer;
	position: absolute;
	top: 0.25rem;
	line-height: 0.4rem;
	z-index: 9;
	display: block;
}

.left-arrow:hover,
.right-arrow:hover {
	background-color: rgba(0, 0, 0, 0.1);
}

#g-top .hu_c {
	width: 8.4rem;
	height: 1rem;
	/* background-color: pink; */
	position: absolute;
}

#g-top .m-down {
	width: 2.8rem;
	height: 0.74rem;
	float: left;
	padding: 0.1rem;
}

/* 客户服务 */
#g-top ul li.kefu {
	width: 0.83rem;
	height: 0.3rem;
}

#g-top ul li.kefu span {
	padding-left: 0.1rem;
	color: #666;
}

/* 网站导航 */
#g-top ul li.wangzhan {
	width: 0.73rem;
	height: 0.3rem;
	/*border: 1px solid orange;*/
}

#g-top ul li.wangzhan span {
	color: #666;
	padding-left: 0.1rem;
}

#g-top ul li.wangzhan .wangzhanhide {
	width: 2.5rem;
	height: 2.06rem;
	border: 0.01rem solid #ccc;
	background: white;
	float: left;
	position: relative;
	top: 0.08rem;
	left: -1.68rem;
	display: none;
	z-index: 22;
}

#g-top ul li.wangzhan:hover .wangzhanhide {
	display: block;
}

#g-top ul li.wangzhan .tese {
	width: 2.5rem;
	height: 0.28rem;
}

#g-top ul li.wangzhan .tese span {
	font-weight: 700;
	line-height: 0.28rem;
}

#g-top ul li.wangzhan .tesemain {
	width: 2.4rem;
	height: 0.66rem;
	margin-left: 0.05rem;
	border-bottom: 0.01rem solid #eee;
	margin-bottom: 0.05rem;
}

#g-top ul li.wangzhan a {
	font-weight: 400;
	color: #666;
	padding-left: 0.06rem;
	line-height: 1.7em;
}

#g-top ul li.wangzhan a:hover {
	color: #E4393C;
	text-decoration: underline;
}

#g-top ul li.wangzhan .tesemain1,
#g-top ul li.wangzhan .tesemain2 {
	width: 2.4rem;
	height: 0.22rem;
	margin-left: 0.05rem;

}

#g-top ul li.wangzhan .tesemain1 {
	border-bottom: 0.01rem solid #eee;

}

/* 手机京东 */
#g-top ul li.hidetu {
	position: relative;
}

#g-top ul li .erwei {
	width: 2.5rem;
	height: 3.94rem;
	border: 1px solid #ccc;
	float: left;
	position: absolute;
	top: 0.4rem;
	z-index: 33;
	background: white;
	display: none;
}

#g-top ul li.hidetu:hover .erwei {
	display: block;
}

#g-top ul li .erwei .bo {
	width: 1.09rem;
	height: 1.09rem;
	margin-top: 0.2rem;
	margin-left: 0.1rem;
	border: 0.01rem solid #CCCCCC;
}

#g-top ul li .erwei .shoudan,
#g-top ul li .erwei .jd,
#g-top ul li .erwei .ewtu {
	width: 1rem;
	height: 1rem;
	margin: 0.02px auto;
}

#g-top ul li .erwei .shoudan {
	background: url(../img/wei01.png) no-repeat;
	background-size: 100%;
}

#g-top ul li .erwei .jd {
	background-size: 100%;
	background: url(../img/wei02.jpg) no-repeat;
}

#g-top ul li .erwei .ewtu {
	background: url(../img/wei03.jpg) no-repeat;
	background-size: 100%;
}

#g-top ul li .erwei .span1 {
	width: 1rem;
	font-size: 0.12rem;
	font-weight: 700;
	color: #E4393C;
	display: block;
	float: left;
	margin-top: -1rem;
	margin-left: 1.5rem;
}

#g-top ul li .erwei .apple {
	width: 0.96rem;
	height: 0.28rem;
	background: url(../img/all.png) no-repeat 0px -3.6rem;
	margin-left: 1.3rem;
	margin-top: -0.75rem;
}

#g-top ul li .erwei .andr {
	width: 0.96rem;
	height: 0.28rem;
	background: url(../img/all.png) no-repeat 0px -3.99rem;
	margin-left: 1.3rem;
	margin-top: 0.1rem;
}



#g-top ul li .downjian1 {
	width: 0.1rem;
	height: 0.08rem;
	background: url(../img/all.png) no-repeat -0.92rem -0.53rem;
	position: relative;
	top: -0.13rem;
	left: 0.68rem;
}

#g-top ul li .kefuhide {
	width: 0.78rem;
	height: 1.5rem;
	margin-left: 0.05rem;
	margin-top: -0.02rem;
	position: relative;
	z-index: 999;
}

#g-top ul li .kefuhide ul li {
	width: 0.73rem;
	height: 0.22rem;
	margin-left: 0.05rem;
}

#g-top ul li .a1 {
	width: 0.83rem;
	height: 0.2rem;
	overflow: hidden;
	position: relative;
	z-index: 999;
}

#g-top ul li.kefu:hover .a1 {
	overflow: visible;
}

#g-top ul li.kefu:hover .kefuhide {
	border: 0.01rem solid #ccc;
	background: white;
}