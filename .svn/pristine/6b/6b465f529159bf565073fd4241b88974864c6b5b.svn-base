	vertical-align: middle;
	cursor: pointer
}
.kon-hz {
	width: 30%;
	margin: 0 auto
}
.kon-box .kon-wz {
	display: block;
	font-size: 1rem;
	color: #aaa;
	margin: 20px 0
}

.kon-box .kon-lj {
	display: inline-block;
	width: 100%;
	height: 2rem;
	line-height: 2rem;
	background: #383838;
