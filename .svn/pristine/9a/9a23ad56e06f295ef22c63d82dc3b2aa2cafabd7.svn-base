* {
	margin: 0;
	padding: 0;
}

body,
html {
	font-size: 100px;
}

ul li {
	list-style: none;
}



/* 头部导航加logo */
#m-head {
	font-size: 0.2rem;
}

#m-head ul li {
	margin-top: 0.15rem;
}

#m-head ul li a:hover {
	color: #E1251B;
}

/* 搜索 */
#m-head .sou {
	float: left;
	width: 2.1rem;
	height: 0.57rem;
	margin-top: 0.2rem;
	margin-left: 0.2rem;
	/*border: 1px solid orange;*/
}

#m-head .sou .sousuo {
	width: 2.1rem;
	height: 0.4rem;
	float: left;
	margin-top: 0.04rem;
	border: 0.03rem solid #E4393C;
}

#m-head .sou .kuang {
	width: 1.25rem;
	height: 0.35rem;
	float: left;
	padding: 0.04rem 0.04rem;
	line-height: 0.2rem;
	border: none;
}

#m-head .sou .souzi {
	width: 0.78rem;
	height: 0.35rem;
	float: right;
	background: #E4393C;
}

#m-head .sou .souzi a {
	display: block;
	font-size: 0.16rem;
	font-weight: 700;
	color: white;
	text-align: center;
	line-height: 0.35rem;
}


#m-head .gouwuche {
	width: 1.42rem;
	height: 0.4rem;
	border: 0.01rem solid orange;
	float: left;
	background: #F7F7F7;
	border: 0.01rem solid #ccc;
	margin-top: 0.22rem;
	margin-left: 0.35rem;
	position: relative;
	font-size: 0.14rem;
}

#m-head .gouwuche .chetu {
	width: 0.22rem;
	height: 0.2rem;
	background: url(../img/all.png) no-repeat -1.2rem -0.88rem;
	position: absolute;
	top: 0.05rem;
	left: 0.08rem;
}

#m-head .gouwuche a {
	display: block;
	padding-left: 0.3rem;
	padding-top: 0.09rem;
	color: #666;
}

#m-head .gouwuche a:hover {
	color: #E4393C;
	text-decoration: underline;
}

#m-head .gouwuche .jianleft {
	width: 0.1rem;
	height: 0.1rem;
	background: url(../img/all.png) no-repeat -1.1rem -0.45rem;
	position: absolute;
	top: 0.15rem;
	right: 0.05rem;
}

#m-head .gouwuche .num {
	width: 0.17rem;
	height: 0.2rem;
	position: absolute;
	top: -0.1rem;
	left: 0.2rem;
	background: url(../img/all.png) no-repeat -1.34rem -2.8rem;
}

#m-head .gouwuche .num .numright {
	width: 0.1rem;
	height: 0.2rem;
	float: right;
	/*border: 1px solid orange;*/
	background: url(../img/all.png) no-repeat -1.32rem -2.51rem;
}

#m-head .gouwuche .num .numzi {
	width: 0.1rem;
	height: 0.1rem;
	position: absolute;
	top: 0rem;
}

#m-head .gouwuche .num span {
	display: block;
	margin-left: 0.03rem;
	margin-top: -0.02rem;
	font-size: 0.14rem;
	color: #FFF;
}

#m-head .gouwuche .hideche {
	width: 3.48rem;
	height: 0.69rem;
	background: white;
	border: 0.01rem solid #ccc;
	position: absolute;
	top: 0.4rem;
	left: -2.06rem;
	display: none;
	z-index: 22;
}

#m-head .gouwuche:hover .hideche {
	display: block;
}

#m-head .gouwuche .hideche .kongche {
	width: 0.56rem;
	height: 0.49rem;
	margin-left: 0.4rem;
	margin-top: 0.07rem;
	float: left;
	background: url(../img/all.png) no-repeat 0rem 0rem;
}

#m-head .gouwuche .hideche span {
	display: block;
	margin-top: 0.3rem;
	color: #999;
}

/* 主体 */
#m-main {
	padding-bottom: 0.5rem;
	background-color: #f0f3ef;
	font-size: 0.14rem;
}

/* 左侧 */
#m-main .m-m_left ul {
	padding: 0.15rem 0.2rem;
	color: #333333;
	border-bottom: 0.01rem solid #f3f3f3;
}

#m-main .m-m_left ul li a {
	color: #333333;
	line-height: 0.3rem;
}

#m-main .m-m_left ul li a:hover {
	text-decoration: none;
	color: #E1251B;
}

/* 右侧 */
#m-main .m-pic {
	margin-bottom: 0;
	width: 2.5rem;
	height: 2.43rem;
	box-shadow: 0.02rem 0.02rem 0.1rem rgba(0, 0, 0, .3);
}

#m-main .m-pic .mc {
	position: relative;
	height: 1.87rem;
	text-align: center;
	background-color: #e2231a;
	overflow: hidden;
}

#m-main .m-pic .mc .mc-bef {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 0.6rem;
	background: url(../img/c-01.png) no-repeat 0 0;
}

#m-main .m-pic .mc .ma {
	display: block;
	position: relative;
	width: 0.64rem;
	height: 0.64rem;
	top: 0.2rem;
	z-index: 2;
	margin: 0.2rem auto;
	border-radius: 100%;
	border: 0.04rem solid #fff;
	box-shadow: 0.03rem 0.02rem 0.1rem 0 rgba(79, 5, 1, .84);
	overflow: hidden;
}

#m-main .m-pic .mc p a,
#m-main .m-pic .mc p a:hover {
	display: inline-block;
	font-size: 0.12rem;
	padding: 0 0.1rem;
	margin: 0 0.05rem;
	border: 0.01rem solid #fff;
	color: #fff;
	height: 0.22rem;
	line-height: 0.22rem;
	border-radius: 0.12rem;
}

#m-main .m-pic .mc p a:hover {
	text-decoration: none;
	background: #FFFFFF;
	color: #E1251B;
}

#m-main .m-pic .mb {
	padding: 0.03rem 0.1rem;
	/* height: 0.56rem; */
	background-color: #232331;
}

#m-main .m-pic .mb ul li {
	margin-top: 0.05rem;
}

#m-main .m-pic .mb ul li p {
	margin: 0rem;
	color: #f4ca3a;
	font-size: 0.13rem;
}

#m-main .mb ul li a,
#m-main .mb ul li a:hover {
	float: right;
	padding: 0 0.1rem;
	margin-top: -0.35rem;
	height: 0.26rem;
	line-height: 0.26rem;
	border-radius: 0.14rem;
	color: #232331;
	font-weight: 700;
	text-align: center;
	text-decoration: none;
}

#m-main .mb ul li a {
	background-color: #f4ca3a;
}

#m-main .mb ul li a:hover {
	background-color: #FFFFFF;
	text-decoration: none;
}

#m-main .m-mon li {
	float: left;
	text-align: center;
	font-size: 0.13rem;
}

#m-main .m-mon li:not(:first-child) {
	margin-left: 0.82rem;
}

#m-main .m-mon li p:first-child {
	font-size: 0.22rem;
	font-weight: bold;
	padding-top: 0.1rem;
}

#m-main .m-mon li a {
	display: inline-block;
	margin-top: 0.2rem;
	color: #4184e8;
}

#m-main .m-mon li a:hover {
	text-decoration: none;
	color: #E1251B;
}

/* 中间 */
#m-main .my-order li,
#m-main .m-follow li {
	float: left;
}

#m-main .my-order li a {
	width: 0.9rem;
	border: none;
	text-align: center;
	margin-left: 0.25rem;
	font-size: 0.13rem;
}

#m-main .my-order li a span {
	display: inline-block;
	margin: 0.03rem auto;
	background-image: url(../img/yan.png);
	width: 0.44rem;
	height: 0.41rem;
}

#m-main .my-order li:nth-of-type(1) a span {
	background-position: 0rem -2rem;
}

#m-main .my-order li:nth-of-type(2) a span {
	background-position: -0.8rem -2rem;
}

#m-main .my-order li:nth-of-type(3) a span {
	background-position: -1.6rem -2rem;
}

#m-main .my-order li:nth-of-type(4) a span {
	background-position: -2.4rem -2rem;
}

#m-main .my-order li:nth-of-type(5) a span {
	background-position: -3.2rem -2rem;
}

#m-main .my-order li:nth-of-type(1) a:hover span {
	background-position: 0rem -2.5rem;
}

#m-main .my-order li:nth-of-type(2) a:hover span {
	background-position: -0.8rem -2.5rem;
}

#m-main .my-order li:nth-of-type(3) a:hover span {
	background-position: -1.6rem -2.5rem;
}

#m-main .my-order li:nth-of-type(4) a:hover span {
	background-position: -2.4rem -2.5rem;
}

#m-main .my-order li:nth-of-type(5) a:hover span {
	background-position: -3.2rem -2.5rem;
}

#m-main .my-order li a:hover,
#m-main .m-follow li a:hover {
	text-decoration: none;
}

#m-main .m-follow {
	display: flex;
	justify-content: space-between;
	padding: 0.2rem;
}

#m-main .m-follow li {
	height: 0.6rem;
	line-height: 0.3rem;
	font-size: 0.16rem;
}

#m-main .m-follow li a {
	color: #333;
}

#m-main .m-follow li a:hover {
	color: #E1251B;
}

#m-main .m-plus {
	position: relative;
	margin-bottom: 0.2rem;
	background-color: #f5ca3a;
	font-weight: 700;
}

#m-main .plus {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 1.1rem;
	background-image: url(../img/x-01.png);
	background-size: 100%;
}

#m-main .m-plus p a {
	float: right;
	padding: 0.05rem;
	background-color: #2c2e40;
	color: #f5ca3a;
	border-radius: 0.14rem;
	text-align: center;

}
#m-main .pl{
	width: 6.2rem;
	height: 2.6rem;
	padding: 0.1rem;
	position: relative;
	overflow: hidden; 
	
}
#m-main .plus-ulist{
	float: left;
	width: 12rem;
	position: absolute;
	
	
}

#m-main .plus-ulist ul li {
	width: 1.3rem;
	height: 2.2rem;
	float: left;
	background: white;
	padding: 0.1rem 0.18rem;
	font-size: 0.12rem;
	margin-left: 0.18rem;
}
#m-main .plus-ulist ul li a,#m-main .m-live ul li a{
	color: #666;
	text-decoration: none;
}
#m-main .pl .left-arrow,#m-main .m-live .left-arrow {
	left: 0rem;
	background: url(../img/left-arrow-down.PNG) no-repeat scroll center center transparent;
}
#m-main .pl .right-arrow,#m-main .m-live .right-arrow{
	right: 0rem;
	background: url(../img/right-arrow-down.PNG) no-repeat scroll center center transparent;
}
#m-main .pl .left-arrow,#m-main .m-live .left-arrow,
#m-main .pl .right-arrow,#m-main .m-live .right-arrow {
	width: 0.2rem;
	height: 0.4rem;
	cursor: pointer;
	position: absolute;
	top: 1rem;
	line-height: 0.4rem;
	z-index: 9;
	display: block;
}

#m-main .left-arrow:hover,
#m-main .right-arrow:hover {
	background-color: rgba(0, 0, 0, 0.1);
}
/* 生活服务 */
/* #m-main .m-live{
	position: relative;
	
	background: red;
} */
#m-main .m-live ul li{
	width: 0.96rem;
	float: left;
	height: 0.80rem;
	text-align: center;
	border: 0.01rem solid #eee;
	margin-bottom: 0.2rem;
}

#m-main .m-live .l-main ul li a span{
	display: inline-block;
	
	width: 0.4rem;
	height: 0.4rem;
	background-image: url(../img/sprite2.png);

}

#m-main .m-live .l-main li:nth-of-type(1) a span {
	background-position: -2.4rem 0rem;
}

#m-main .m-live .l-main li:nth-of-type(2) a span {
	background-position: -2.8rem 0rem;
}

#m-main .m-live .l-main li:nth-of-type(3) a span {
	background-position: -3.2rem 0rem;
}

#m-main .m-live .l-main li:nth-of-type(4) a span {
	background-position: -3.6rem 0rem;
}

#m-main .m-live .l-main li:nth-of-type(5) a span {
	background-position: -4rem 0rem;
}
#m-main .m-live .l-main li:nth-of-type(6) a span {
	background-position: -4.4rem 0rem;
}

#m-main .m-live .l-main li:nth-of-type(1) a:hover span {
	background-position: -2.4rem 2.7rem;
}

#m-main .m-live .l-main li:nth-of-type(2) a:hover span {
	background-position: -2.8rem 2.7rem;
}

#m-main .m-live .l-main li:nth-of-type(3) a:hover span {
	background-position: -3.2rem 2.7rem;
}

#m-main .m-live .l-main li:nth-of-type(4) a:hover span {
	background-position: -3.6rem 2.7rem;
}

#m-main .m-live .l-main li:nth-of-type(5) a:hover span {
	background-position: -4rem 2.7rem;
}
#m-main .m-live .l-main li:nth-of-type(6) a:hover span {
	background-position: -4.4rem 2.7rem;
}
#m-main .m-buy a:hover{
	text-decoration: none;
	color: #E1251B;
}